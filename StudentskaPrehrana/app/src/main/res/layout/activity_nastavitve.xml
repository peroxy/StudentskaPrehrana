<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_nastavitve"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.fri.studentskaprehrana.NastavitveActivity">

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_saladBar"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_lunch"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_lunch"
        android:layout_centerHorizontal="true"
        android:textOn="ON SAL"
        android:textOff="OFF SAL" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_vegetarian"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_saladBar"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_saladBar"
        android:layout_alignParentEnd="true"
        android:textOn="ON VEG"
        android:textOff="OFF VEG" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_disabledWC"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_disabled"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_disabled"
        android:layout_alignStart="@+id/activity_nastavitve_tb_saladBar"
        android:textOn="ON DWC"
        android:textOff="OFF DWC" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_pizzas"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_disabledWC"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_disabledWC"
        android:layout_alignParentEnd="true"
        android:textOn="ON PIZ"
        android:textOff="OFF PIZ" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_studentBenefits"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_weekends"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_weekends"
        android:layout_alignStart="@+id/activity_nastavitve_tb_disabledWC"
        android:textOn="ON STB"
        android:textOff="OFF STB" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_delivery"
        android:layout_alignBaseline="@+id/activity_nastavitve_tb_studentBenefits"
        android:layout_alignBottom="@+id/activity_nastavitve_tb_studentBenefits"
        android:layout_alignParentEnd="true"
        android:textOn="ON DEL"
        android:textOff="OFF DEL" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_weekends"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:textOn="ON WEE"
        android:textOff="OFF WEE"/>

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_disabled"
        android:layout_above="@+id/activity_nastavitve_tb_studentBenefits"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="22dp"
        android:textOn="ON DIS"
        android:textOff="OFF DIS" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_tb_lunch"
        android:layout_marginBottom="26dp"
        android:layout_above="@+id/activity_nastavitve_tb_disabled"
        android:layout_alignParentStart="true"
        android:textOn="ON LUN"
        android:textOff="OFF LUN" />

    <SeekBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/activity_nastavitve_sb_radius"
        android:layout_above="@+id/activity_nastavitve_tb_lunch"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="96dp"
        android:layout_alignParentEnd="true"
        android:max="100"
        android:progress="10"/>

    <Button
        android:text="Seznam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/btSeznam" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btSeznam"
        android:layout_alignEnd="@+id/activity_nastavitve_tb_lunch"
        android:id="@+id/textView" />

</RelativeLayout>
